{
    "_config_": {
        "codeSrc": "column-object/column.AUTO.json"
    },
    "events": {
        "meta": {
            "displayName": "Events on Column",
            "isEvent": true,
            "suppressMissingPropCheck": true
        },
        "filterActiveChanged": {
            "description": "The filter active value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "sortChanged": {
            "description": "The sort value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "leftChanged": {
            "description": "The left position has changed (e.g. column has moved).",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "movingChanged": {
            "description": "The column has started / finished moving (i.e. user is dragging the column to a new location).",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "widthChanged": {
            "description": "The width value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "visibleChanged": {
            "description": "The visibility value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "menuVisibleChanged": {
            "description": "The column menu was shown / hidden.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "columnRowGroupChanged": {
            "description": "The row group value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "columnPivotChanged": {
            "description": "The pivot value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "columnValueChanged": {
            "description": "The 'value' value has changed.",
            "type": {
                "arguments": {
                    "event": "ColumnEvent"
                },
                "returnType": "void"
            }
        },
        "columnHeaderMouseOver": {
            "description": "A mouse cursor is initially moved over a column header.",
            "type": {
                "arguments": {
                    "event": "ColumnHeaderMouseOverEvent"
                },
                "returnType": "void"
            }
        },
        "columnHeaderMouseLeave": {
            "description": "A mouse cursor is moved out of a column header.",
            "type": {
                "arguments": {
                    "event": "ColumnHeaderMouseLeaveEvent"
                },
                "returnType": "void"
            }
        },
        "columnHeaderClicked": {
            "description": "A column header was clicked.",
            "type": {
                "arguments": {
                    "event": "ColumnHeaderClickedEvent"
                },
                "returnType": "void"
            }
        },
        "columnHeaderContextMenu": {
            "description": "A context menu action, such as right-click or context menu key press, was performed on a column header.",
            "type": {
                "arguments": {
                    "event": "ColumnHeaderContextMenuEvent"
                },
                "returnType": "void"
            }
        }
    }
}