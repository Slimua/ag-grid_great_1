{
  "name": "ag-grid-charts-enterprise",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-grid-charts-enterprise/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "inputs": [],
      "outputs": [],
      "dependsOn": [
        "build:types",
        "build:package",
        "build:umd"
      ]
    },
    "build:updateWithCoreExports": {
      "dependsOn": [
        "ag-grid-community:build"
      ],
      "cache": true,
      "command": "node updateMainTsWithCoreExports.js",
      "options": {
        "cwd": "packages/ag-grid-charts-enterprise"
      }
    },
    "build:types": {
      "dependsOn": ["^build:types"],
      "command": "npx gulp copy-from-module-source && cp main.d.ts.src dist/types/main.d.ts",
      "options": {
        "cwd": "packages/ag-grid-charts-enterprise"
      },
      "cache": true
    },
    "build:umd": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx run ag-grid-charts-enterprise:build:umd:development"]
      },
      "configurations": {
        "production": {
          "commands": ["nx run ag-grid-charts-enterprise:build:umd:production"]
        }
      }
    },
    "build:umd:development": {
      "executor": "@nx/webpack:webpack",
      "inputs": [],
      "outputs": [
        "{projectRoot}/dist/ag-grid-charts-enterprise.js"
      ],
      "dependsOn": [
        "^build",
        "build:updateWithCoreExports",
        "build:copy-styles"
      ],
      "options": {
        "webpackConfig": "packages/ag-grid-charts-enterprise/webpack.config.js",
        "outputPath": "ignore",
        "outputFileName": "ignore"
      },
      "cache": true,
      "configurations": {
        "production": {
          "buildTarget": "build:umd:production"
        }
      }
    },
    "build:umd:production": {
      "executor": "nx:run-commands",
      "inputs": [],
      "outputs": ["{projectRoot}/dist/*.js"],
      "cache": false,
      "dependsOn": ["^build", "build:updateWithCoreExports", "build:copy-styles"],
      "options": {
        "parallel": true,
        "commands": [
          "npx webpack --env production --env minify --env styles       --env entry=./src/main-styles.ts",
          "npx webpack --env production              --env styles       --env entry=./src/main-styles.ts",
          "npx webpack --env production --env minify --env styles=false --env entry=./src/main.ts",
          "npx webpack --env production              --env styles=false --env entry=./src/main.ts"
        ],
        "cwd": "packages/ag-grid-charts-enterprise"
      }
    },
    "build:copy-styles": {
      "dependsOn": ["@ag-grid-community/styles:build"],
      "inputs": [],
      "cache": true,
      "command": "rsync -r ../../community-modules/styles/*.*css --delete styles",
      "options": {
        "cwd": "packages/ag-grid-charts-enterprise"
      }
    },
    "build:package": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": [
        "^build",
        "build:updateWithCoreExports"
      ],
      "inputs": [
        "tsDefaults",
        {
          "dependentTasksOutputFiles": "**/main.cjs.js",
          "transitive": true
        }
      ],
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/ag-grid-charts-enterprise/dist/package",
        "main": "packages/ag-grid-charts-enterprise/src/main.ts",
        "tsConfig": "packages/ag-grid-charts-enterprise/tsconfig.lib.json",
        "external": ["ag-grid-community"],
        "esbuildConfig": "esbuild.config.cjs",
        "platform": "browser",
        "target": "es6",
        "format": [
          "cjs",
          "esm"
        ],
        "sourcemap": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "packages/ag-grid-charts-enterprise/tsconfig.watch.json"
        },
        "production": {
          "sourcemap": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "community-modules/jest.config.ts"
      }
    }
  },
  "tags": [
    "package:enterprise",
    "package",
    "enterprise"
  ]
}
