{
  "name": "ag-grid-community",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-grid-community/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "inputs": [],
      "outputs": [],
      "dependsOn": ["build:types", "build:package", "build:umd", "build:copy-styles"]
    },
    "build:updateWithCoreExports": {
      "dependsOn": ["@ag-grid-community/core:build"],
      "cache": true,
      "command": "node updateMainTsWithCoreExports.js",
      "options": {
        "cwd": "packages/ag-grid-community"
      }
    },
    "build:umd": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "@ag-grid-community/core:build",
        "@ag-grid-community/client-side-row-model:build",
        "@ag-grid-community/csv-export:build",
        "@ag-grid-community/infinite-row-model:build"
      ],
      "options": {
        "commands": ["nx run ag-grid-community:build:umd:development"]
      },
      "configurations": {
        "production": {
          "commands": ["nx run ag-grid-community:build:umd:production"]
        }
      }
    },
    "build:umd:development": {
      "executor": "@nx/webpack:webpack",
      "inputs": [],
      "outputs": [
        "{projectRoot}/dist/ag-grid-community.js"
      ],
      "dependsOn": [
        "^build",
        "build:updateWithCoreExports",
        "build:copy-styles"
      ],
      "options": {
        "webpackConfig": "packages/ag-grid-community/webpack.config.js",
        "outputPath": "ignore",
        "outputFileName": "ignore"
      },
      "cache": true,
      "configurations": {
        "production": {
          "buildTarget": "build:umd:production"
        }
      }
    },
    "build:umd:production": {
      "executor": "nx:run-commands",
      "inputs": [],
      "outputs": ["{projectRoot}/dist/*.js"],
      "cache": false,
      "dependsOn": [
        "^build",
        "build:updateWithCoreExports",
        "build:copy-styles"
      ],
      "options": {
        "parallel": true,
        "commands": [
          "npx webpack --env production --env minify --env styles       --env entry=./src/main-styles.ts",
          "npx webpack --env production              --env styles       --env entry=./src/main-styles.ts",
          "npx webpack --env production --env minify --env styles=false --env entry=./src/main.ts",
          "npx webpack --env production              --env styles=false --env entry=./src/main.ts"
        ],
        "cwd": "packages/ag-grid-community"
      }
    },
    "build:copy-styles": {
      "dependsOn": ["@ag-grid-community/styles:build"],
      "cache": true,
      "command": "rsync -r ../../community-modules/styles/*.*css --delete styles",
      "options": {
        "cwd": "packages/ag-grid-community"
      }
    },
    "build:types": {
      "dependsOn": [
        "@ag-grid-community/core:build:types",
        "@ag-grid-community/client-side-row-model:build:types",
        "@ag-grid-community/csv-export:build:types",
        "@ag-grid-community/infinite-row-model:build:types"
      ],
      "command": "npx gulp copy-from-module-source && cp main.d.ts.src dist/types/main.d.ts",
      "options": {
        "cwd": "packages/ag-grid-community"
      },
      "cache": true
    },
    "build:package": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": [
        "build:updateWithCoreExports",
        "^build"
      ],
      "inputs": [
        "tsDefaults",
        {
          "dependentTasksOutputFiles": "**/main.cjs.js",
          "transitive": true
        }
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/ag-grid-community/dist/package",
        "main": "packages/ag-grid-community/src/main.ts",
        "tsConfig": "packages/ag-grid-community/tsconfig.lib.json",
        "esbuildConfig": "esbuild.config.cjs",
        "platform": "browser",
        "target": "es6",
        "format": ["cjs", "esm"],
        "sourcemap": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "packages/ag-grid-community/tsconfig.watch.json"
        },
        "production": {
          "sourcemap": false
        }
      }
    }
  },
  "tags": ["package:community", "package", "community"]
}















