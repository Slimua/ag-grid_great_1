{
  "name": "ag-grid-enterprise",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ag-grid-enterprise/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "inputs": [],
      "outputs": [],
      "dependsOn": [
        "build:types",
        "build:package",
        "build:umd"
      ]
    },
    "build:updateWithCoreExports": {
      "dependsOn": [
        "ag-grid-community:build"
      ],
      "cache": true,
      "command": "node updateMainTsWithCoreExports.js",
      "options": {
        "cwd": "packages/ag-grid-enterprise"
      }
    },
    "build:types": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build:types"],
      "inputs": [],
      "options": {
        "parallel": true,
        "commands": [
          "mkdir -p dist/types && rsync -r ../../community-modules/core/dist/types/src/ --delete dist/types/core",
          "mkdir -p dist/types && rsync -r ../../community-modules/client-side-row-model/dist/types/src/ --delete dist/types/client-side-row-model",
          "mkdir -p dist/types && rsync -r ../../community-modules/infinite-row-model/dist/types/src/ --delete dist/types/infinite-row-model",
          "mkdir -p dist/types && rsync -r ../../community-modules/csv-export/dist/types/src/ --delete dist/types/csv-export",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/core/dist/types/src/ --delete dist/types/enterprise-core",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/advanced-filter/dist/types/src/ --delete dist/types/advanced-filter",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/charts/dist/types/src/ --delete dist/types/charts",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/clipboard/dist/types/src/ --delete dist/types/clipboard",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/column-tool-panel/dist/types/src/ --delete dist/types/column-tool-panel",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/excel-export/dist/types/src/ --delete dist/types/excel-export",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/filter-tool-panel/dist/types/src/ --delete dist/types/filter-tool-panel",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/master-detail/dist/types/src/ --delete dist/types/master-detail",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/menu/dist/types/src/ --delete dist/types/menu",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/multi-filter/dist/types/src/ --delete dist/types/multi-filter",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/range-selection/dist/types/src/ --delete dist/types/range-selection",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/rich-select/dist/types/src/ --delete dist/types/rich-select",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/row-grouping/dist/types/src/ --delete dist/types/row-grouping",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/server-side-row-model/dist/types/src/ --delete dist/types/server-side-row-model",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/set-filter/dist/types/src/ --delete dist/types/set-filter",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/side-bar/dist/types/src/ --delete dist/types/side-bar",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/sparklines/dist/types/src/ --delete dist/types/sparklines",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/status-bar/dist/types/src/ --delete dist/types/status-bar",
          "mkdir -p dist/types && rsync -r ../../enterprise-modules/viewport-row-model/dist/types/src/ --delete dist/types/viewport-row-model",
          "mkdir -p dist/types && cp main.d.ts.src dist/types/main.d.ts"
        ],
        "cwd": "packages/ag-grid-enterprise"
      },
      "cache": true
    },
    "build:umd": {
      "executor": "@nx/webpack:webpack",
      "inputs": [],
      "outputs": [
        "{projectRoot}/dist/ag-grid-enterprise.js"
      ],
      "dependsOn": [
        "^build",
        "build:updateWithCoreExports",
        "build:copy-styles"
      ],
      "options": {
        "webpackConfig": "packages/ag-grid-enterprise/webpack.config.js",
        "outputPath": "overridden in webpack config but required by nx",
        "outputFileName": "overridden in webpack config but required by nx"
      },
      "cache": true
    },
    "build:copy-styles": {
      "dependsOn": [],
      "inputs": [],
      "cache": true,
      "command": "rsync -r ../../community-modules/styles/*.*css --delete styles",
      "options": {
        "cwd": "packages/ag-grid-enterprise"
      }
    },
    "build:package": {
      "executor": "@nx/esbuild:esbuild",
      "dependsOn": [
        "^build",
        "build:updateWithCoreExports"
      ],
      "inputs": [
        "tsDefaults"
      ],
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/ag-grid-enterprise/dist/package",
        "main": "packages/ag-grid-enterprise/src/main.ts",
        "tsConfig": "packages/ag-grid-enterprise/tsconfig.lib.json",
        "external": ["ag-grid-community"],
        "esbuildConfig": "esbuild.config.cjs",
        "platform": "browser",
        "target": "es6",
        "format": [
          "cjs",
          "esm"
        ],
        "sourcemap": true
      },
      "configurations": {
        "watch": {
          "tsConfig": "packages/ag-grid-enterprise/tsconfig.watch.json"
        },
        "production": {
          "sourcemap": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "community-modules/jest.config.ts"
      }
    }
  },
  "tags": [
    "package:enterprise",
    "package",
    "enterprise"
  ]
}
